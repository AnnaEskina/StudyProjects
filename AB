df_funnel_2_group.columns = ['event_name','A','B']
ylist = list(df_funnel_2_group['event_name'])
fig = go.Figure()
for ev in df_funnel_2_group.columns[1:4]:
    xlist = list(df_funnel_2_group[ev]) 
    fig.add_trace(go.Funnel(name = '–ì—Ä—É–ø–ø–∞ ' + ev,
            y = ylist,
            x = xlist,
            textinfo = "value+percent previous+percent initial"))            
